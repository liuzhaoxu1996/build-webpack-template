{
	"name": "<%= name %>",
	"version": "1.0.0",
	"description": "<%= description %>",
	"author": "<%= author %>",
	"scripts": {
		"so:dev": "webpack --watch --config sobuild/sobuild.dev.js",
		"so:serve": "webpack serve --config sobuild/sobuild.dev.js --open 'Google Chrome'",
		"so:prod": "webpack --watch --config sobuild/sobuild.prod.js"
	},
	"devDependencies": {
		"glob": "^7.1.6",
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"babel-loader": "^8.1.0",
		"cross-env": "^5.0.1",
		"clean-webpack-plugin": "^3.0.0",
		"webpack": "^5.6.0",
		"webpack-cli": "^4.1.0",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.1.5",
		"eslint": "^7.13.0",
		<% if(webpack.['file-loader']) { %>
		"file-loader": "^6.2.0",
		<% } %>
		"webpack-dev-server": "^3.11.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"@q/sofe-eslint": "^1.0.6",
		"@q/sofe-stylelint": "^1.0.7",
		<% if(webpack['css-minimizer-webpack-plugin']) { %>
		"css-minimizer-webpack-plugin": "^1.1.5",
		<% } %>
		<% if(webpack['html-webpack-plugin']) { %>
		"html-webpack-plugin": "^4.5.0",
		<% } %>
		<% if(webpack['less-loader']) { %>
		"less-loader": "^7.1.0",
		<% } %>
		<% if(webpack['mini-css-extract-plugin']) { %>
		"mini-css-extract-plugin": "^1.3.1",
		<% } %>
		<% if(webpack['purgecss-webpack-plugin']) { %>
		"purgecss-webpack-plugin": "^3.0.0",
		<% } %>
		<% if(webpack['purgecss-webpack-plugin']) { %>
		"purgecss-webpack-plugin": "^3.0.0",
		<% } %>
		<% if(webpack['terser-webpack-plugin']) { %>
		"terser-webpack-plugin": "^5.0.3",
		<% } %>
		<% if(webpack['ts-loader']) {%>
		"ts-loader": "^8.0.11",
		<% } %>
		<% if(webpack['postcss-loader']) {%>
		"postcss-loader": "4.1.0",
		<% } %>
		"webpack-merge": "^5.4.0"
	},
	"engines": {
		"node": ">= 6.0.0",
		"npm": ">= 3.0.0"
	},
	"browserslist": [
		"defaults"
	]
}